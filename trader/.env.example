LIGHTER_BASE_URL=https://mainnet.zklighter.elliot.ai

GRVT_ENV=prod
GRVT_END_POINT_VERSION=v1
GRVT_WS_STREAM_VERSION=v1

# Auth 
AUTH_JWT_SECRET=super-secret-change-me
AUTH_TOKEN_TTL_MINUTES=10080
AUTH_LOCKOUT_THRESHOLD=3
AUTH_LOCKOUT_MINUTES=60
ADMIN_REGISTRATION_SECRET=replace-with-random-admin-secret
ADMIN_CLIENT_HEADER_NAME=X-Admin-Client-Secret
USER_CACHE_TTL_SECONDS=300
CRYPTO_KEY=your-base64-32-byte-key

# Postgres (for Tortoise/Aerich)
PGHOST=your-postgres-hostname
PGDATABASE=your_db_name
PGUSER=your_db_user
PGPASSWORD=your_db_password
PGSSLMODE=require
PGCHANNELBINDING=require
# DATABASE_URL=postgresql+asyncpg://your_db_user:your_db_password@ep-your-neon-hostname-pooler.region.aws.neon.tech:5432/your_db_name
